def bubble_sort(array):
  for i in range(0,len(array)):
    flag=False
    for j in range(0,len(array)-i-1):
      if array[j]>array[j+1]:
        temp=array[j]
        array[j]=array[j+1]
        array[j+1]=temp
        flag=True
    if not flag:break
  print(f"your array after bubble sort:"array)
        
